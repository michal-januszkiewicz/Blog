<div class="col-md-4">
  <%= link_to user.name, user %>
</div>
<div class="col-md-4">
  <%= user.email %>
</div>
<div class="col-md-4">
  <% if current_user.admin? && (current_user != user) %>
    <%= link_to "delete", user, method: :delete,
                data: { confirm: "You sure?" } %>
  <% end %>
  <% if current_user == user %>
    <%= link_to "edit", edit_user_registration_path %>
  <% end %>
</div>
