<div class="col-md-4">
  <%= link_to user.name, user %>
</div>
<div class="col-md-4">
  <%= user.email %>
</div>
<div class="col-md-4">
  <% if current_user.admin? && (current_user != user) %>
    <%= link_to t(:delete), user, method: :delete,
                data: { confirm: t(:you_sure) } %>
  <% end %>
  <% if current_user == user %>
    <%= link_to t(:edit), edit_user_registration_path %>
  <% end %>
</div>
